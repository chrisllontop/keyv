{
  "name": "@keyv/compress-gzip",
  "version": "2.0.0",
  "description": "gzip compression for keyv",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts --format cjs,esm --dts --clean",
    "prepare": "yarn build",
    "test": "xo --fix && vitest run --coverage",
    "test:ci": "xo && vitest --run --sequence.setupFiles=list",
    "clean": "rm -rf node_modules && rm -rf .nyc_output && rm -rf ./dist && rm -rf coverage.lcov && rm -rf ./test/testdb.sqlite"
  },
  "xo": {
    "rules": {
      "import/no-named-as-default": "off",
      "import/extensions": "off",
      "@typescript-eslint/prefer-nullish-coalescing": "off",
      "@typescript-eslint/no-unsafe-argument": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredwray/keyv.git"
  },
  "keywords": [
    "compress",
    "gzip",
    "keyv",
    "storage",
    "adapter",
    "key",
    "value",
    "store",
    "cache",
    "ttl"
  ],
  "author": "Jared Wray <me@jaredwray.com> (http://jaredwray.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaredwray/keyv/issues"
  },
  "homepage": "https://github.com/jaredwray/keyv",
  "dependencies": {
    "@types/pako": "^2.0.3",
    "pako": "^2.1.0",
    "@keyv/serialize": "*"
  },
  "devDependencies": {
    "@keyv/test-suite": "*",
    "tsd": "^0.31.1",
    "xo": "^0.59.3"
  },
  "tsd": {
    "directory": "test"
  },
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "dist",
    "LICENSE"
  ]
}
